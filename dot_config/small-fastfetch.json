{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small"
  },
  "display": {
    "key": {
      "type": "icon"
    },
    "bar": {
      "charElapsed": "=",
      "charTotal": "-",
      "width": 13
    },
    "percent": {
      "type": 7
    },
    "separator": "  "
  },
  "modules": [
    {
      "type": "title",
      "color": {
        "user": "green",
        "at": "red",
        "host": "blue"
      }
    },
    "separator",
    "os",
    "kernel",
    "uptime",
    "memory",
    {
      "type": "disk"
    },
    {
      "type": "battery",
      "key": "Battery"
    },
    {
      "type": "colors",
      "paddingLeft": 1,
      "symbol": "circle",
      "key": "Colors"
    }
  ]
}
